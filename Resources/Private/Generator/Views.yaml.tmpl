-
  requestFilter: 'isPackage("{packageKey}")'
  viewObjectName: 'byTorsten\React\Core\View\ReactView'
  <f:if condition="{path}">options:
    reactClientFilePattern: 'resource://{packageKey}/Private/{path}/index.client.js'
    reactServerFilePattern: 'resource://{packageKey}/Private/{path}/index.server.js'</f:if>
